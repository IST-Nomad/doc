#Настройка со стороны Eltex:
configure
security ike proposal cisco_ike_prop
encryption algorithm aes128
dh-group 14
authentication algorithm sha1
exit

security ike policy cisco_ike_pol
pre-shared-key ascii-text 7#cNd*GS5VRg*Tj_3sV=QGYC
proposal cisco_ike_prop
exit

no security ike gateway cisco_ike_gw
security ike gateway IRK_MOLOKO_EDGE_R2
ike-policy cisco_ike_pol
mode route-based
bind-interface vti 2
mobike disable
exit

security ipsec proposal cisco_ipsec_prop
encryption algorithm aes128
authentication algorithm sha1
exit

security ipsec policy cisco_ipsec_pol
proposal cisco_ipsec_prop
exit

no security ipsec vpn cisco_ipsec
security ipsec vpn IRK_MOLOKO_EDGE_R2_IPSEC
mode ike
ike establish-tunnel route
ike gateway IRK-MOLOKO-EDGE-R2
ike ipsec-policy cisco_ipsec_pol
enable
exit

tunnel vti 2
  description "IRK-MOLOKO-EDGE-R2"
  mtu 1400
  ip firewall disable
  ip netflow export ingress
  local address 195.206.62.4
  remote address 62.141.98.154
  ip address 172.16.139.17/30
  ip ospf instance 1
  ip ospf area 0.0.0.1
  ip ospf network point-to-point
  ip ospf
  enable
exit

#Настройка со стороны Cisco:

crypto isakmp policy 20
encryption aes 128
authentication pre-share
group 14
hash sha
exit
crypto ipsec transform-set ipsec-policy-Eltex esp-aes 128 esp-sha-hmac
mode tunnel
exit
crypto ipsec df-bit clear
crypto keyring Eltex-KEYRING
pre-shared-key address 195.206.62.4 key 7#cNd*GS5VRg*Tj_3sV=QGYC
exit
crypto isakmp profile Eltex
keyring Eltex-KEYRING
match identity address 195.206.62.4 255.255.255.255
exit
crypto ipsec profile ipsec-profile-Eltex
set transform-set ipsec-policy-Eltex 
set isakmp-profile Eltex
exit
interface Tunnel3
description IRK-HO-R3
bandwidth 100000
ip address 172.16.139.18 255.255.255.252
ip mtu 1400
ip tcp adjust-mss 1300
tunnel source FastEthernet4
tunnel mode ipsec ipv4
tunnel destination 195.206.62.4
tunnel protection ipsec profile ipsec-profile-Eltex
exit
router ospf 1
network 172.16.139.16 0.0.0.3 area 0.0.0.1
