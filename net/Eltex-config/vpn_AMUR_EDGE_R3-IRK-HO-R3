#AMUR-EDGE-R3
configure
tunnel vti 2
description "Eltex-IRK-HO-R3_2"
mtu 1400
ip firewall disable
local address 91.142.149.82
remote address 185.242.116.75
ip address 172.16.138.2/30
ip ospf instance 1
ip ospf area 0.0.0.1
ip ospf network point-to-point
ip ospf
enable
exit
security ike gateway IRK_HO_R3_2
ike-policy Eltex_ike_pol
mode route-based
bind-interface vti 2
mobike disable
exit
security ipsec vpn IRK_HO_R3_IPSEC_2
mode ike
ike establish-tunnel route
ike gateway IRK_HO_R3_2
ike ipsec-policy Eltex_ipsec_pol
enable
exit

tunnel vti 2
  description "Eltex-IRK-HO-R3_2"
  mtu 1400
  ip firewall disable
  local address 91.142.149.82
  remote address 185.242.116.75
  ip address 172.16.138.2/30
  ip ospf instance 1
  ip ospf area 0.0.0.1
  ip ospf network point-to-point
  ip ospf
  enable
exit

#IRK-HO-R3
configure

no security ike gateway AMUR_EDGE_R1
no security ipsec vpn AMUR_EDGE_R1_IPSEC
no tunnel vti 9

security ike gateway AMUR_EDGE_R3_2
ike-policy Eltex_ike_pol
mode route-based
bind-interface vti 9
mobike disable
exit
security ipsec vpn AMUR_EDGE_R3_IPSEC_2
mode ike
ike establish-tunnel route
ike gateway AMUR_EDGE_R3_2
ike ipsec-policy Eltex_ipsec_pol
enable
exit
tunnel vti 9
  description "AMUR-EDGE-R3"
  mtu 1400
  ip firewall disable
  local address 185.242.116.75
  remote address 91.142.149.82
  ip address 172.16.138.1/30
  ip ospf instance 1
  ip ospf area 0.0.0.1
  ip ospf cost 1
  ip ospf network point-to-point
  ip ospf
  enable
exit



